{
  "name": "e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for Firebase Functions",
  "main": "run_tests.js",
  "scripts": {
    "clear-db": "node clear_database.js",
    "setup-users": "node setup_users.js",
    "set-custom-claims": "node set_custom_claims.js",
    "get-tokens": "node get_tokens.js",
